                {
                    "title": "Fuel scored",
                    "description": "The amount of fuel scored in autonomous",
                    "type": "counter",
                    "required": false,
                    "code": "FIA",
                    "formResetBehavior": "reset",
                    "defaultValue": 0,
                    "min": 0,
                    "step": 1
                },
                {
                    "title": "Climbed Tower",
                    "description": "The height the robot climbed in autonomous(Level 1 = Bottom Rung, Level 2 = Middle Rung, Level 3 = Top Rung)",
                    "type": "select",
                    "code": "climbedAuto",
                    "required": false,
                    "defaultValue": "No",
                    "formResetBehavior": "reset",
                    "choices": {
                        "No": "Didn't Climb",
                        "L1": "Level 1",
                        "L2": "Level 2",
                        "L3": "Level 3",
                        "F": "Failed Climb"
                    }
                },
                {
                    "title": "Auto Fouls",
                    "type": "counter",
                    "required": false,
                    "code": "auf",
                    "formResetBehavior": "reset",
                    "defaultValue": 0,
                    "min": 0,
                    "step": 1
                }
            ]
