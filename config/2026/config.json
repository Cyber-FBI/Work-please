    "sections": [
        {
            "name": "Prematch",
            "fields": [
                {
                    "title": "Scouter Initials",
                    "description": "Enter the initials of the scouter.",
                    "type": "text",
                    "required": true,
                    "code": "scouter",
                    "formResetBehavior": "preserve",
                    "defaultValue": ""
                },
                {
                    "title": "Match Number",
                    "description": "Enter the match number.",
                    "type": "number",
                    "required": true,
                    "code": "matchNumber",
                    "formResetBehavior": "increment",
                    "defaultValue": 1
                },
                {
                    "title": "Robot",
                    "description": "The robot you are scouting this match, based on driver station position.",
                    "type": "select",
                    "required": true,
                    "code": "robot",
                    "formResetBehavior": "preserve",
                    "defaultValue": "R1",
                    "choices": {
                        "R1": "Red 1",
                        "R2": "Red 2",
                        "R3": "Red 3",
                        "B1": "Blue 1",
                        "B2": "Blue 2",
                        "B3": "Blue 3"
                    }
                },
                {
                    "title": "Team Number",
                    "description": "The team number of the robot you're scouting.",
                    "type": "number",
                    "required": true,
                    "code": "teamNumber",
                    "formResetBehavior": "reset",
                    "defaultValue": 0,
                    "min": 0,
                    "max": 19999
                },
                {
                    "title": "Starting Position",
                    "description": "The starting position of the robot.",
                    "type": "select",
                    "required": true,
                    "code": "Prsp",
                    "formResetBehavior": "reset",
                    "defaultValue": "",
                    "choices": {
                        "R1": "Outpost Side",
                        "R2": "Middle",
                        "R3": "Depot Side"
                    }
                },
                {
                    "title": "No Show",
                    "description": "Check if the robot did not show up for the match.",
                    "type": "boolean",
                    "required": false,
                    "code": "noShow",
                    "formResetBehavior": "reset",
                    "defaultValue": false
                }
            ]
        },
        {
            "name": "Autonomous",
            "fields": [
                {
                    "title": "Moved?",
                    "description": "Check if the robot moved during autonomous.",
                    "type": "boolean",
                    "required": false,
                    "code": "Mved",
                    "formResetBehavior": "reset",
                    "defaultValue": false
                },
                {
                    "title": "Moved To?",
                    "description": "How far a robot moved in autonomous",
                    "type": "select",
                    "required": false,
                    "code": "MTA",
                    "formResetBehavior": "reset",
                    "defaultValue": "",
                    "choices": {
                        "R1": "Didn't Move",
                        "R2": "Stayed in Alliance Zone",
                        "R3": "Moved to Neutral Zone",
                        "R4": "Moved to Opponent Zone"
                        }
                },
                {
                    "title": "Method of Crossing Zones",
                    "description": "How did a robot move to the other zones in autonomous",
                    "type": "select",
                    "required": false,
                    "code": "MCZ",
                    "formResetBehavior": "reset",
                    "defaultValue": "",
                    "choices": {
                        "R1": "Didn't Cross Zones",
                        "R2": "Under Trench",
                        "R3": "Over Bump",
                        "R4": "Both"
                        }
                },
                {
                    "title": "Fuel Collected From Ground",
                    "description": "Collected fuel from the ground in autonomous",
                    "type": "boolean",
                    "defaultValue": false,
                    "code": "FFGA",
                    "formResetBehavior": "reset",
                    "required": false
                },
                {
                    "title": "Fuel Collected From Player",
                    "description": "Collected fuel from the human playerin autonomous",
                    "type": "boolean",
                    "defaultValue": false,
                    "code": "FFHPA",
                    "formResetBehavior": "reset",
                    "required": false
                },
                {
                    "title": "Fuel Collected From Depot",
                    "description": "Collected fuel from the depotin autonomous",
                    "type": "boolean",
                    "defaultValue": false,
                    "code": "FFDA",
                    "formResetBehavior": "reset",
                    "required": false
